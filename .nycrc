{
  "extends": "@istanbuljs/nyc-config-typescript",
  "all": true,
  "check-coverage": true,
  "lines": 80,
  "statements": 80,
  "functions": 80,
  "branches": 80,
  "include": [
    "out/src/**/*.js"
  ],
  "exclude": [
    "out/test/**",
    "out/src/test/**",
    "**/*.test.js",
    "**/*.d.ts"
  ],
  "reporter": [
    "html",
    "text",
    "text-summary"
  ],
  "report-dir": "coverage"
}
